{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "arkhamdb",
  "title": "ArkhamDB",
  "description": "Search Arkham Horror LCG decks and cards",
  "icon": "command-icon.png",
  "author": "funkymonkeymonk",
  "categories": [
    "Fun"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "card",
      "title": "Search Cards",
      "description": "Search cards in ArkhamDB",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.39.1",
    "@raycast/utils": "^1.1.0",
    "got": "^12.3.1",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
